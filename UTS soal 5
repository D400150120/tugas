#include <stdio.h>
#include <math.h>

float fungsiku( int x, int m)
{
	float y;
	//proses
	y = sqrt( (5*pow(x,3)) - pow(x,m-1) + 12 );
	return y;
}

int main() {
	int a;
	int b;
	float y = 0;
	printf("Masukkan Nilai x = ",a); scanf("%d",&a);
	printf("Masukkan Nilai m = ",b); scanf("%d",&b);
	y = fungsiku(a,b);
	printf("nilai x=%d, nilai m=%d, hasilnya y=%f \n",a,b,y);
	return 0;
}
